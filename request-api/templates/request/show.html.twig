{% extends 'base.html.twig' %}

{% block title %}Request{% endblock %}

{% block body %}
    <h1>Request</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Uuid</th>
                <td>{{ request.uuid }}</td>
            </tr>
            <tr>
                <th>AgentId</th>
                <td>{{ request.agentId }}</td>
            </tr>
            <tr>
                <th>RequesterId</th>
                <td>{{ request.requesterId }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_domain_entity_request_index') }}">back to list</a>

    <a href="{{ path('app_domain_entity_request_edit', {'uuid': request.uuid}) }}">edit</a>

    {{ include('request/_delete_form.html.twig') }}
{% endblock %}
